{
  "manifest_version": 3,
  "name": "ValidDog - OpenAPI Validator",
  "version": "1.0.0",
  "description": "DevToolsでAPIトラフィックをOpenAPI仕様書に基づいてバリデーション",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "devtools_page": "devtools.html"
}

